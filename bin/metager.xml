<?xml version="1.0" encoding="UTF-8"?>
<rendertheme xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" map-background="#F8F8F8"
    map-background-outside="#DDDDDD" version="5" xmlns="http://mapsforge.org/renderTheme"
    xsi:schemaLocation="http://mapsforge.org/renderTheme https://raw.githubusercontent.com/mapsforge/mapsforge/master/resources/renderTheme-v6.xsd">
    <!-- General Looks of Water and Land -->
	<rule e="way" k="natural" v="issea|sea">
        <area fill="#a3ccff" ></area>
    </rule>
    <rule e="way" k="natural" v="nosea">
        <area fill="#f0ede5" stroke="#f0ede5" stroke-width="1.0" />
    </rule>
    <rule e="way" k="natural" v="land">
        <area fill="#F8F8F8" stroke="#f0ede5" stroke-width="0.1" />
    </rule>
    <rule e="way" k="landuse" v="military">
        <area fill="#dfdbd4" stroke="#dfdbd4"  />
    </rule>


<!-- landuse -->

    <rule e="way" k="landuse" v="residential" zoom-min="10">
        <area fill="#dfdfdf" />
    </rule>
    <rule e="way" k="landuse" v="retail" zoom-min="14">
        <area fill="#dfdfdf" stroke="#dfdfdf" stroke-width="0.2" />
    </rule>

    <rule e="way" k="landuse" v="commercial|industrial|brownfield|railway|garages|construction"
        zoom-min="14">
        <area fill="#dfdfdf" stroke="#dfdfdf" stroke-width="0.2" />
    </rule>
    <rule e="way" k="amenity" v="school|college|university" zoom-min="14">
        <area fill="#dfdfdf" stroke="#dfdfdf" stroke-width="0.2" />
    </rule>
    <rule e="way" k="leisure|tourism|landuse"
        v="park|zoo|picnic_site|camp_site|caravan_site|recreation_ground" zoom-min="14">
        <area fill="#cbe6a3" stroke="#cbe6a3" stroke-width="0.2" />
    </rule>
    <rule e="way" k="natural|landuse" v="forest|wood">
        <area fill="#d2e4c8" />
        <rule e="any" k="*" v="*" zoom-min="13">
            <rule e="any" k="wood" v="coniferous">
                <area src="assets/patterns/coniferous.svg" symbol-height="50" symbol-width="50" />
            </rule>
            <rule e="any" k="wood" v="deciduous">
                <area src="assets/patterns/deciduous.svg" symbol-height="50" symbol-width="50" />
            </rule>
            <rule e="any" k="wood" v="~|*">
                <area src="assets/patterns/coniferous_and_deciduous.svg" symbol-height="50"
                    symbol-width="50" />
            </rule>
        </rule>
    </rule>

    <rule e="way" k="landuse|natural" v="meadow|grassland" zoom-min="13">
        <area fill="#dde6d5" stroke="#dde6d5" stroke-width="0.1" />
    </rule>
    

    <rule e="way" k="natural" v="beach|sand" zoom-min="14">
        <area fill="#faf2c7" />
    </rule>
    <rule e="way" k="natural" v="rock|stone|bare_rock|bedrock" zoom-min="14">
        <area fill="#BBF1F1F1" stroke="#F1F1F1" stroke-width="0.1" />
        <area src="assets/patterns/hills.svg" symbol-height="40" symbol-width="40" />
    </rule>
    <rule e="way" k="natural" v="heath" zoom-min="14">
        <area fill="#e8eddb" stroke="#e8eddb" stroke-width="0.1" />
        <area src="assets/patterns/grass.svg" symbol-percent="30" />
    </rule>
    <rule e="way" k="natural" v="scrub" zoom-min="14">
        <area fill="#d2e4c8" stroke="#d2e4c8" stroke-width="0.1" />
        <area src="assets/patterns/scrub.svg" symbol-percent="50" />
    </rule>
    <rule e="way" k="natural" v="fell" zoom-min="14">
        <area fill="#BBE9FCE6" stroke="#E9FCE6" stroke-width="0.1" />
        <area src="assets/patterns/quarry.svg" symbol-height="90" symbol-width="90" />
    </rule>
    <rule e="way" k="natural" v="scree" zoom-min="14">
        <area fill="#BBF5F5F5" stroke="#F5F5F5" stroke-width="0.1" />
        <area src="assets/patterns/quarry.svg" symbol-height="90" symbol-width="90" />
    </rule>
    <rule e="way" k="natural" v="glacier" zoom-min="12">
        <area fill="#BBE0FFFF" stroke="#E0FFFF" stroke-width="0.8" />
    </rule>
    <rule e="way" k="landuse" v="landfill|quarry" zoom-min="14">
        <area fill="#F8F8F8" stroke="#F8F8F8" stroke-width="0.2" />
        <area src="assets/patterns/quarry.svg" symbol-height="90" symbol-width="90" />
    </rule>
    <rule e="way" k="landuse|leisure" v="garden|village_green|common|grass" zoom-min="14">
        <area fill="#cbe6a3" stroke="#cbe6a3" stroke-width="0.2" />
    </rule>
    <rule e="way" k="landuse|leisure" v="allotments|greenfield|golf_course" zoom-min="15">
        <area fill="#d2e4c8" stroke="#d2e4c8" stroke-width="0.2" />
    </rule>
    <rule e="way" k="landuse|amenity" v="cemetery|grave_yard" zoom-min="14">
    	<area fill="#d6e9b9" stroke="#d6e9b9" stroke-width="0.2" />
    </rule>
    <rule e="way" k="natural|landuse"
        v="land|beach|sand|rock|fell|meadow|grassland|heath|marsh|wetland|scree|scrub|forest|wood"
        zoom-min="17">
        <caption fill="#7e7e7e" font-family="sans_serif" font-size="10" font-style="italic" k="name"
            stroke="#FFFFFF" stroke-width="0.1" />
    </rule>
    <rule e="way" k="landuse" v="landfill|quarry|vineyard|field|farm|farmland|orchard"
        zoom-min="16">
        <caption fill="#7e7e7e" font-family="sans_serif" font-size="16" font-style="italic" k="name"
            stroke="#FFFFFF" stroke-width="0.1" />
    </rule>
    <rule e="way" k="landuse|leisure|amenity"
        v="grass|village_green|recreation_ground|military|park|garden|common|green|dog_park|allotments|cemetery|graveyard"
        zoom-min="16">
        <caption fill="#7e7e7e" font-family="sans_serif" font-size="14" font-style="italic" k="name"
            stroke="#FFFFFF" stroke-width="2.0" />
    </rule>


<!-- buildings -->
    <rule e="any" k="*" v="*"><!--
        <rule e="way" k="waterway" v="dock" zoom-min="14">
            <area fill="#ff0000" />
            <line stroke="#CCCCCC" stroke-linecap="butt" stroke-width="0.5" />
        </rule>-->
        <rule e="way" k="lock" v="yes|true" zoom-min="14">
            <line stroke="#a3ccff" stroke-linecap="square" stroke-width="2.0" />
        </rule><!--
        <rule e="way" k="waterway" v="weir" zoom-min="14">
            <line stroke="#ff0000" stroke-width="0.7" />
            <line stroke="#ff0000" stroke-dasharray="5,1" stroke-linecap="butt"
                stroke-width="0.7" />
        </rule>
        <rule e="node" k="waterway" v="weir" zoom-min="16">
            <symbol src="jar:symbols/water/weir.svg" />
        </rule>-->
        <rule e="way" k="waterway" v="dam" zoom-min="14">
           
            <rule closed="yes" e="any" k="*" v="*">
                <area fill="#faedd5" />
            </rule>
        </rule>
        <rule e="way" k="building" v="*" zoom-min="17">
            <area fill="#d9d0c9" stroke="#c2b5aa" stroke-width="0.2" />
        </rule>
        <rule e="way" k="landuse|area" v="~">
            <rule e="way" k="amenity|tourism|railway|shop|information"
                v="place_of_worship|shelter|alpine_hut|restaurant|hotel|station|supermarket|convenience|beverages|bakery|doityourself|sports|bicycle|car|toilets|hostel|office|doctors|theatre">
                <area fill="#f3e9d7" stroke="#e9d6b1" stroke-width="0.2" />
            </rule>
        </rule>
        <rule e="way" k="building" v="*" zoom-min="17">
            <rule e="way" k="historic" v="ruins|castle|memorial|monument">
                <area fill="#f8ecd5" stroke="#e9d6b1" stroke-width="0.2" />
            </rule>
        </rule><!--
        <rule e="way" k="barrier" v="fence" zoom-min="15">
            <line stroke="#ff0000" stroke-dasharray="2,2" stroke-linecap="butt"
                stroke-width="0.1" />
        </rule>--><!--
        <rule e="way" k="barrier|building" v="wall|city_wall" zoom-min="15">
            <line stroke="#ff0000" stroke-linecap="round" stroke-width="0.3" />
            <line stroke="#ff0000" stroke-dasharray="5,1" stroke-linecap="butt"
                stroke-width="0.15" />
        </rule>--><!--
        <rule e="way" k="barrier" v="retaining_wall" zoom-min="15">
            <line stroke="#333333" stroke-linecap="round" stroke-width="0.5" />
            <line stroke="#EEEEEE" stroke-dasharray="4,1" stroke-linecap="butt"
                stroke-width="0.3" />
        </rule>-->
    </rule>

<!-- tunnels -->
    <rule e="way" k="tunnel" v="true|yes|culvert|building_passage|passage">
        <!-- highway tunnel -->
        <rule e="way" k="highway" v="steps|footway|path">
            <line stroke="#b8b394" stroke-dasharray="4,1" stroke-linecap="butt"
                    stroke-width="0.2" />
        </rule>
        <rule e="way" k="highway" v="track|bridleway|byway|cycleway|service|road|pedestrian|unclassified|residential|living_street|construction">
            <line stroke="#FFFFFF" stroke-width="1" />
        </rule>

        <rule e="way" k="highway" v="tertiary|tertiary_link|secondary_link|primary_link|trunk_link|motorway_link|secondary|primary|trunk">
             <line stroke="#FFFFFF" stroke-width="1.5" />
        </rule>
        <rule e="way" k="highway" v="motorway">
            <line stroke="#eab671" stroke-width="1.7" />
            <line stroke="#fed89d" stroke-width="1.6" />
        </rule>

        <!-- railway tunnel -->
        <rule e="way" k="railway"
            v="tram|subway|light_rail|narrow_gauge|preserved|funicular|miniature|monorail">
            <line stroke="#d7d7d7" stroke-dasharray="1,1" stroke-linecap="butt"
                stroke-width="0.35" />
        </rule>
        <rule e="way" k="railway" v="rail">
            <line stroke="#d7d7d7" stroke-dasharray="1,1" stroke-linecap="butt"
                stroke-width="0.35" />
        </rule>
    </rule>


	<!-- highway cores -->
    <rule e="way" k="tunnel" v="~|no|false">
        <!-- no area -->
        <rule e="way" k="area" v="~|no|false">
            <!-- highway cores -->
            <rule e="way" k="highway" v="footway" zoom-min="14">
                <line stroke="#b8b394" stroke-dasharray="4,1" stroke-linecap="butt"
                    stroke-width="0.2" />
            </rule>
            <rule e="way" k="highway" v="path" zoom-min="14">
                <line stroke="#b8b394" stroke-dasharray="4,1" stroke-linecap="butt"
                    stroke-width="0.2" />
            </rule>
            <rule e="way" k="highway" v="steps" zoom-min="14">
                <line stroke="#888888" stroke-linecap="butt" stroke-width="0.2" />
                <line stroke="#DDDDDD" stroke-dasharray="3,3" stroke-linecap="butt"
                    stroke-width="0.2" />
            </rule>
            <rule e="way" k="highway" v="track|byway">
                <rule e="way" k="tracktype" v="grade1" zoom-min="14">
                    <line stroke="#b8b394" stroke-width="0.2" />
                </rule>
                <rule e="way" k="tracktype" v="grade2" zoom-min="14">
                    <line stroke="#b8b394" stroke-width="0.2" stroke-dasharray="25,5" stroke-linecap="butt" />
                </rule>
                <rule e="way" k="tracktype" v="grade3" zoom-min="14">
                    <line stroke="#b8b394" stroke-dasharray="15,10" stroke-linecap="butt"
                        stroke-width="0.2" />
                </rule>
                <rule e="way" k="tracktype" v="grade4" zoom-min="14">
                    <line stroke="#b8b394" stroke-dasharray="10,10" stroke-linecap="butt"
                        stroke-width="0.2" />
                </rule>
                <rule e="way" k="tracktype" v="grade5" zoom-min="14">
                    <line stroke="#b8b394" stroke-dasharray="5,15" stroke-linecap="butt"
                        stroke-width="0.2" />
                </rule>
                <rule e="way" k="tracktype" v="~" zoom-min="14">
                    <line stroke="#b8b394" stroke-dasharray="15,5,5,5" stroke-linecap="butt"
                        stroke-width="0.2" />
                </rule>
            </rule>
            <rule e="way" k="highway" v="bridleway" zoom-min="16">
                <line stroke="#00b300" stroke-dasharray="5,10" stroke-linecap="round"
                    stroke-width="0.2" />
            </rule>
            <rule e="way" k="highway" v="cycleway" zoom-min="15">
                <line stroke="#b8b394" stroke-width="0.3" />
            </rule>
            <rule e="way" k="highway" v="service" zoom-min="16">
                <line stroke="#FFFFFF" stroke-width="1" />
            </rule>
            <rule e="way" k="highway" v="construction" zoom-min="16">
            	<area stroke="#FFFFFF" />
                <line stroke="#878787" stroke-dasharray="7,10" stroke-linecap="butt"
                    stroke-width="1" />
            </rule>
            <rule e="way" k="highway" v="pedestrian" zoom-min="14">
                <line stroke="#FFFFFF" stroke-width="1.2" />
            </rule>
            <rule e="way" k="highway" v="unclassified|residential|living_street|road" zoom-min="14">
                <line stroke="#FFFFFF" stroke-width="1.2" />
            </rule>
            <rule e="way" k="highway" v="tertiary|tertiary_link">
                <line stroke="#FFFFFF" stroke-width="1.5" />
            </rule>
            <rule e="way" k="highway" v="secondary_link">
                <line stroke="#FFFFFF" stroke-width="1.5" />
            </rule>
            <rule e="way" k="highway" v="primary_link">
                <line stroke="#FFFFFF" stroke-width="1.5" />
            </rule>
            <rule e="way" k="highway" v="trunk_link">
                <line stroke="#fde7c4" stroke-width="1.5" />
            </rule>
            <rule e="way" k="highway" v="motorway_link">
                <line stroke="#fde3b9" stroke-width="1.5" />
            </rule>
            <rule e="way" k="highway" v="secondary">
                <line stroke="#FFFFFF" stroke-width="1.5" />
            </rule>
            <rule e="way" k="highway" v="primary">
            	<line stroke="#efda9e" stroke-width="1.5" />
                <line stroke="#ffebaf" stroke-width="1.4" />
            </rule>
            <rule e="way" k="highway" v="trunk">
                <line stroke="#efda9e" stroke-width="1.5" />
                <line stroke="#ffebaf" stroke-width="1.4" />
            </rule>
            <rule e="way" k="highway" v="motorway">
                <line stroke="#eab671" stroke-width="1.7" />
                <line stroke="#fed89d" stroke-width="1.6" />
            </rule>
        </rule>
    </rule>

    <!-- highway captions etc. -->
    <rule e="way" k="tunnel" v="~|no|false"><!---->
        <rule e="way" k="highway" v="footway|path|track|bridleway|steps|byway" zoom-min="7">
            <pathText display="always" fill="#7e7e7e" font-family="sans_serif" font-size="12" font-style="normal" k="name"
                priority="-8" stroke="#FFFFFF" stroke-width="0.25" />
        </rule>
        <rule e="way" k="highway"
            v="cycleway|service|construction|road|pedestrian|unclassified|residential|living_street|tertiary|tertiary_link"
            zoom-min="7">
            <pathText display="always" fill="#7e7e7e" font-family="sans_serif" font-size="12" font-style="normal" k="name"
                priority="-7" stroke="#FFFFFF" stroke-width="0.25" />
        </rule>
        <rule e="way" k="highway"
            v="secondary_link|primary_link|trunk_link|motorway_link|secondary|primary"
            zoom-min="7">
            <pathText display="always" fill="#7e7e7e" font-family="sans_serif" font-size="12" font-style="normal" k="name"
                priority="-6" stroke="#FFFFFF" stroke-width="0.25" />
        </rule>
        <rule e="any" k="highway" v="secondary|tertiary" zoom-min="10">
        	<rule e="way" k="name" v="~">
                <pathText fill="#7e7e7e" font-size="10" font-family="sans_serif" font-style="normal" k="ref" priority="-5"
                    stroke="#FFFFFF" stroke-width="1" />
            </rule>
        </rule>
        <rule e="any" k="highway" v="trunk|primary" zoom-min="9">
        	<rule e="way" k="name" v="~">
                <pathText fill="#7e7e7e" font-family="sans_serif" font-size="12" font-style="normal" k="ref" priority="-5"
                    stroke="#FFFFFF" stroke-width="0.25" />
            </rule>
        </rule>
        <rule e="any" k="highway" v="motorway" zoom-min="7">
            <pathText display="always" fill="#3878c7" repeat="true" repeat-gap="0.0000001f" font-family="sans_serif" font-size="12" font-style="normal" k="ref" priority="-4"
                stroke="#FFFFFF" stroke-width="3.0" />
        </rule>
        <rule e="way" k="highway" v="trunk|primary|secondary|tertiary|secondary_link|primary_link|trunk_link|motorway_link|secondary|primary|service|construction|road|unclassified|residential|living_street|tertiary|tertiary_link" zoom-min="17">
            <!--<rule e="way" k="access" v="destination">
                <line src="jar:patterns/access-destination.png" stroke-width="1.0" />
            </rule>
            <rule e="way" k="access" v="private">
                <line src="jar:patterns/access-private.png" stroke-width="1.0" />
            </rule>TODO!!!!!!!!!!!!!!!!!-->
            <rule e="way" k="oneway" v="yes|true|1" zoom-min="16">
                <lineSymbol align-center="true" display="always" priority="-20" repeat="true" symbol-scaling="size" symbol-height="20" symbol-width="20"
                    src="assets/symbols/oneway.svg" />
            </rule>
        </rule>
    </rule>


  <!-- waterbodies -->
    <rule e="any" k="*" v="*">
        <!-- Couldn't find any of this on the german map
        <rule e="way" k="bridge" v="yes|true|viaduct|aqueduct|suspension|culvert|swing">
            <rule e="way" k="waterway" v="ditch|drain|stream">
                <line stroke="#ff0000" stroke-linecap="butt" stroke-width="1.6" />
                <line stroke="#ff0000" stroke-linecap="butt" stroke-width="1.2" />
            </rule>
            <rule e="way" k="waterway" v="canal|river">
                <line stroke="#ff0000" stroke-linecap="butt" stroke-width="2.0" />
                <line stroke="#ff0000" stroke-linecap="butt" stroke-width="1.6" />
            </rule>
        </rule>
        <rule e="way" k="waterway" v="ditch">
            <line stroke="#ff0000" stroke-width="0.6" />
        </rule>-->
        <rule e="way" k="waterway" v="drain" zoom-min="13">
            <line stroke="#a3ccff" stroke-linecap="butt" stroke-width="0.3" />
        </rule>
        <rule e="way" k="waterway" v="canal" zoom-min="13">
            <line stroke="#a3ccff" stroke-linecap="butt" stroke-width="1.1" />
        </rule>
        <rule e="way" k="waterway" v="stream" zoom-min="13">
            <line stroke="#a3ccff" stroke-width="0.6" />
        </rule>
        <rule e="way" k="waterway" v="river" zoom-min="10">
            <line stroke="#a3ccff" stroke-width="1.1" />
        </rule>
        <rule e="way" k="waterway" v="riverbank">
            <area fill="#a3ccff" />
        </rule>
        <rule e="way" k="waterway" v="river" zoom-min="9">
            <pathText display="always" fill="#6C96E6" font-family="sans_serif" font-size="14" font-style="italic" k="name"
                priority="-100" stroke="#FFFFFF" stroke-width="2.0" />
        </rule>
        <rule e="way" k="waterway" v="stream|canal" zoom-min="15">
            <pathText display="always" fill="#6C96E6" font-family="sans_serif" font-size="12" font-style="italic" k="name"
                priority="-100" stroke="#FFFFFF" stroke-width="2.0" />
        </rule>
        <rule e="way" k="natural|landuse" v="water|reservoir|basin">
            <area fill="#a3ccff" />
            <rule e="any" k="*" v="*" zoom-min="15">
                <caption display="always" fill="#6C96E6" font-size="12" k="name" priority="-100"
                    stroke="#FFFFFF" stroke-width="2.0" />
            </rule>
        </rule>
    </rule>

    <!-- non-physical boundaries -->
    <!-- administrative borders -->
    <rule e="any" k="*" v="*">
        <rule e="way" k="admin_level" v="2">
            <rule e="any" k="*" v="*" >
                <line stroke="#666666" stroke-linecap="butt" stroke-width="1.2" />
                <pathText display="always" fill="#606060" font-size="12" font-style="bold" k="name" priority="-8" stroke="#FFFFFF" stroke-width="2" />
            </rule>
        </rule>
        <rule e="way" k="admin_level" v="4" zoom-min="7">
            <line stroke="#b4b3b1" stroke-dasharray="5, 5" stroke-width="0.75" />
        </rule>
        <!-- Smaller Districts won't be shown yet. We can change that later on maybe
        <rule e="way" k="admin_level" v="6">
            <line stroke="#70EDC2EC" stroke-linecap="butt" stroke-width="4" />
            <line stroke="#693937" stroke-dasharray="5, 5" stroke-width="0.25" />
        </rule>
        <rule e="way" k="admin_level" v="8">
            <line stroke="#70EDC2EC" stroke-dasharray="15, 5, 5, 5" stroke-linecap="butt"
                stroke-width="4" />
            <line stroke="#693937" stroke-dasharray="15, 5, 5, 5" stroke-width="0.25" />
        </rule>
        <rule e="way" k="admin_level" v="9">
            <line stroke="#70EDC2EC" stroke-dasharray="15, 15" stroke-linecap="butt"
                stroke-width="4" />
            <line stroke="#693937" stroke-dasharray="15, 15" stroke-width="0.25" />
        </rule>-->

        <!-- special areas -->
        <rule e="way" k="military" v="*">
            <area fill="#dfdbd4" />
        </rule>
        <!-- nature_reserve and national parks can get marked specially but we will not do so in the default layer --><!--
        <rule e="way" k="leisure|boundary" v="nature_reserve|national_park|protected_area">
            <rule e="any" k="protect_class" v="1|2|3|4|5|6|7|97|98|99|~" zoom-min="15">
                <area fill="#40C0FFBF" />

                <caption fill="#8ABF8A" font-family="serif" font-size="14" font-style="bold_italic"
                    k="name" stroke="#FFFFFF" stroke-width="2.0" />
            </rule>
        </rule>
    -->
    </rule>


<!-- accommodation -->
    <rule e="any" k="*" v="*"><!--
        <rule e="any" k="tourism" v="alpine_hut" zoom-min="12">
            <symbol id="alpine-hut" src="assets/symbols/accommodation/alpinehut.svg" />
            <caption fill="#0092DA" font-size="12" font-style="bold" k="name" stroke="#FFFFFF"
                stroke-width="2.0" symbol-id="alpine-hut" />
            <caption fill="#0092DA" font-size="12" font-style="bold" k="ele" stroke="#FFFFFF"
                stroke-width="2.0" symbol-id="alpine-hut" />
